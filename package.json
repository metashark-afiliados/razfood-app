
{
  "name": "razfood-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Plataforma SaaS Multi-Tenant para la gestión de menús, pedidos y operaciones de restaurantes, construida con una arquitectura de élite.",
  "author": {
    "name": "Raz Podestá - MetaShark Tech",
    "email": "raz.podesta@metashark.tech",
    "url": "https://raz.metashark.tech"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/restoralia.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type:check": "tsc --noEmit",
    "db:types:gen": "supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" --schema public > src/lib/types/database/_supabase.generated.ts",
    "prisma:gen": "prisma generate",
    "prisma:pull": "prisma db pull",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "gen:i18n:manifest": "tsx --tsconfig tsconfig.json scripts/i18n/generate-manifest.ts",
    "gen:i18n:schema": "tsx --tsconfig tsconfig.json scripts/i18n/generate-schema.ts",
    "gen:i18n": "pnpm gen:i18n:manifest && pnpm gen:i18n:schema",
    "gen:icons": "tsx --tsconfig tsconfig.json scripts/generation/generate-lucide-icon-enum.ts",
    "gen:all": "pnpm gen:icons && pnpm gen:i18n",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/next": "^4.0.0",
    "@dnd-kit/core": "^6.3.1",
    "@graphql-tools/merge": "^9.1.1",
    "@hookform/resolvers": "5.2.1",
    "@prisma/client": "^5.17.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@supabase/supabase-js": "^2.56.0",
    "@tanstack/react-table": "8.21.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dataloader": "^2.2.2",
    "dayjs": "1.11.14",
    "framer-motion": "^11.3.19",
    "graphql": "^16.11.0",
    "lucide-react": "0.542.0",
    "next": "14.2.5",
    "next-intl": "^3.17.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.62.0",
    "react-hot-toast": "2.6.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "1.0.7",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "18.3.7",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "husky": "^9.1.4",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
